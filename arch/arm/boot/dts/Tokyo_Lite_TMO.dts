	pinctrl-3 = <&mtkfb_pins_lcd_bias_enn0>; 
	pinctrl-4 = <&mtkfb_pins_lcd_bias_enn1>; 
	pinctrl-5 = <&mtkfb_pins_lcm_reset0>; 
 	pinctrl-6 = <&mtkfb_pins_lcm_reset1>;
	status = "okay";
};
