&i2c1{
	cust_mag@0 {
		compatible		= "mediatek,akmd09918";
		/* i2c_num		= <1>; */
		/* i2c_addr		= <0x0C 0 0 0>; */
		direction		= <3>;
		power_id		= <0xffff>;
		power_vol		= <0>;
		/* is_batch_supported	= <0>; */
	};
	cust_accel@0 { 
        compatible = "mediatek,stk8ba53_dir";
        //i2c_num = <1>;
        direction = <1>;
        power_id = <0xffff>;
        power_vol = <0>;
        firlen = <0>;
        is_batch_supported = <0>;
	};
	cust_alsps@0 {
        compatible = "mediatek,stk3x1x";
        /*i2c_num = <1>;
        i2c_addr = <0x51 0 0 0>;*/
        polling_mode_ps = <0>;
        polling_mode_als = <1>;
        power_id = <0xffff>;
        power_vol = <0>;
        als_level = <0 328 861 1377 3125 7721 7767 12621 23062 28430 33274 47116 57694 57694 65535>;
        als_value = <0 133 304 502 1004 2005 3058 5005 8008 10010 12000 16000 20000 20000 20000 20000>;
        ps_threshold_high = <26>;
        ps_threshold_low = <21>;
        is_batch_supported_ps = <0>;
        is_batch_supported_als = <0>; 	
	};

	gsensor@4c {
                i2c_num = <1>;
                direction = <6>;
                power_id = <0xffff>;
                power_vol = <0>;
                firlen = <16>;
                is_batch_supported = <1>;
	};
	cust_accel@1 {
                compatible = "mediatek,mc34xx";
                //i2c_num       = <1>;
                direction = <1>;
                power_id = <0xffff>;
                power_vol = <0>;
                firlen = <0>;
                is_batch_supported = <0>;
	};
	alsps@46 {
                status = "okay";
                /* customization */
                i2c_num = <1>;
                i2c_addr = <0x46 0 0 0>;
                polling_mode_ps = <0>;
                polling_mode_als = <1>;
                power_id = <0xffff>;
                power_vol = <0>;
                als_level = <0 328 861 1377 3125 7721 7767 12621 23062 28430 33274 47116 57694 57694 65535>;
                als_value = <0 133 304 502 1004 2005 3058 5005 8008 10010 12000 16000 20000 20000 20000 20000>;
                ps_threshold_high       =  <65535>;
                ps_threshold_low        =  <0>;
                is_batch_supported_ps   = <0>;
                is_batch_supported_als  = <0>;
                stk,transmittance = <1000>;
                stk,state-reg = <0x00>;
                stk,psctrl-reg = <0x30>;
                stk,alsctrl-reg = <0x30>;
                stk,ledctrl-reg = <0x60>;
                stk,wait-reg = <0x0C>;
                stk,ps-thdh = <1000>;
                stk,ps-thdl = <900>;
                stk,mmi-thdh-limit = <5000>;
                stk,mmi-thdl-limit = <1>;
                stk,use-fir;
                pinctrl-names = "default","state_en_output0", "state_en_output1";
		pinctrl-0 = <&psensor_pins_default>;
		pinctrl-1 = <&psensor_pins_en_output0>;
		pinctrl-2 = <&psensor_pins_en_output1>;
	};

};
